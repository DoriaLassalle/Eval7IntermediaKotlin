<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ThirdFragment">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="5dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/nameShow"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="textview"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/lastShow"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="textview"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/phoneShow"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:focusable="true"
            android:text="textview"
            android:textColor="#E1051D"
            android:textSize="22sp" />

        <TextView
            android:id="@+id/mailShow"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:focusable="true"
            android:text="textview"
            android:textColor="#E1051D"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/companyShow"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="textview"
            android:textColor="#101010"
            android:textSize="18sp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="50dp" />

        <Button
            android:id="@+id/volverAgenda2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_redondo"
            android:text="@string/volver_a_la_agenda" />


    </LinearLayout>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="133dp"
        android:layout_marginBottom="2dp"
        android:src="@drawable/ic_baseline_perm_contact_calendar_24"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>